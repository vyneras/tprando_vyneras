{
  "tracker_right_map": {
    "type": "container",
    "background": "#212121",
    "content": {
      "type": "dock",
      "content": [
        {
          "type": "dock",
          "dock": "left",
          "max_width": 370,
          "v_alignment": "stretch",
          "margin": "0,0,3,0",
          "dropshadow": true,
          "content": [
            {
              "type": "group",
              "header": "Items",
              "dock": "top",
              "header_content": {
                "type": "button_popup",
                "style": "settings",
                "popup_background": "#FF212121",
                "layout": "settings"
              },
              "content": {
                "scale": 0.4,
                "type": "layout",
                "key": "item_grid_container"
              }
            },
            {
              "type": "group",
              "header": "Dungeons",
              "dock": "top",
              "content": {
                "scale": 0.28,
                "type": "layout",
                "key": "dungeon_grid_container"
              }
            },
            {
              "type": "group",
              "header": "Pinned Locations",
              "content": {
                "type": "scroll",
                "horizontal_scrollbar_visibility": "disabled",
                "vertical_scrollbar_visibility": "hidden",
                "content": [
                  {
                    "type": "recentpins",
                    "style": "wrap",
                    "h_alignment": "left",
                    "v_alignment": "top",
                    "orientation": "horizontal",
                    "compact": true
                  }
                ]
                
              }
            }
          ]
        },
        {
          "type": "dock",
          "content": [
            {
              "type": "tabbed",
              "dock": "top",
              "tabs": [
                {
                  "title": "Overworld",
                  "icon": "images/map.png",
                  "content": {
                    "type": "map",
                    "maps": [
                      "overworld"
                    ]
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  },
  "tracker_top_map": {
    "type": "container",
    "background": "#212121",
    "content": {
      "type": "dock",
      "content": [
        {
          "type": "dock",
          "dock": "bottom",
          "max_height": 240,
          "dropshadow": true,
          "content": [
            {
              "type": "group",
              "header": "Items",
              "dock": "left",
              "header_content": {
                "type": "button_popup",
                "style": "settings",
                "popup_background": "#FF212121",
                "layout": "settings"
              },
              "content": {
                "scale": 0.4,
                "type": "layout",
                "key": "item_grid_container"
              }
            },
            {
              "type": "group",
              "header": "Dungeons",
              "dock": "left",
              "content": {
                "scale": 0.4,
                "type": "layout",
                "key": "dungeon_grid_container"
              }
            },
            {
              "type": "group",
              "header": "Pinned Locations",
              "content": {
                "type": "scroll",
                "horizontal_scrollbar_visibility": "disabled",
                "vertical_scrollbar_visibility": "hidden",
                "content": [
                  {
                    "type": "recentpins",
                    "style": "wrap",
                    "h_alignment": "left",
                    "v_alignment": "top",
                    "orientation": "horizontal",
                    "compact": true
                  }
                ]
                
              }
            }
          ]
        },
        {
          "type": "tabbed",
          "tabs": [
            {
              "title": "Overworld",
              "icon": "images/map.png",
              "content": {
                "type": "map",
                "maps": [
                  "overworld"
                ]
              }
            }
          ]
        }
      ]
    }
  }
}